# CPQ系统前端生产环境配置
# 前端域名：cpq.100yse.com
# 后端域名：cpqh.100yse.com
# 更新时间：2025-08-25

# API后端地址
VITE_API_BASE_URL=http://cpqh.100yse.com/api/v1

# 应用配置
VITE_APP_TITLE=CPQ System - 智能报价系统
VITE_APP_ENV=production
VITE_APP_VERSION=1.0.0

# 生产环境优化配置
VITE_BUILD_COMPRESS=true
VITE_BUILD_ANALYZE=false
VITE_BUILD_SOURCEMAP=false

# 网络配置
VITE_HTTPS=false
VITE_API_TIMEOUT=120000
VITE_UPLOAD_MAX_SIZE=10485760

# CDN和静态资源配置
VITE_CDN_BASE_URL=http://cpqh.100yse.com
VITE_STATIC_BASE_URL=/assets/

# 安全配置
VITE_ENABLE_MOCK=false
VITE_ENABLE_DEVTOOLS=false
VITE_ENABLE_ERROR_LOG=true

# 性能监控
VITE_ENABLE_PWA=false
VITE_ENABLE_GZIP=true